		Press(Mouse.LeftButton);
		Release(Mouse.LeftButton);
		Wait(var.repairWaitTime s);

		Mouse.cursorpos = [Window.Left, var.WindowTop] + [var.scaleX * 227, var.scaleY * 387];
		Wait(var.repairWaitTime s);
		Press(Mouse.LeftButton);
		Release(Mouse.LeftButton);
		Wait(var.repairWaitTime s);

		Mouse.cursorpos = [Window.Left, var.WindowTop] + [var.scaleX * 1900, var.scaleY * 770];
		[var.cursorX, var.cursorY] = Mouse.cursorpos;
	}
}
