*/
} else {
	////////////////////////////////////////////////////////////////////////////////
	// button mapping
	// F5(One), F6(Two), F7(Three), F8(Four): 1~4, F9: cloSing windows , F10: force moving
	// F3(Say 따라오게)
	//
	if (Pressed(Mouse.MiddleButton)) {
		if (var.debug) DebugPrint("[" + Round((Mouse.cursorposX - Window.Left) / var.ScaleX) + ", " + Round((Mouse.cursorposY - var.WindowTop) / var.ScaleY) + "]");
	}
	
	if (Pressed(var.XI_RT > var.dzTR)) Press(Mouse.LeftButton);
	if (Released(var.XI_RT > var.dzTR)) Release(Mouse.LeftButton);

	if (Pressed(var.XI_LT > var.dzTR)) {
		if (var.dealerMode) {
			Wait(0.05 s);
			Press(Mouse.RightButton);
		} else {
			Press(Mouse.RightButton);
		}
	}
	if (Released(var.XI_LT > var.dzTR)) Release(Mouse.RightButton);
	if (var.supportMode) {
		if (var.XI_LT > var.dzTR) {
			Press(Mouse.RightButton);
			Release(Mouse.RightButton);
		}
	}

	if (Pressed(var.XI_A)) Press(Key.One);
	if (Released(var.XI_A)) Release(Key.One);

	if (Pressed(var.XI_X)) Press(Key.Two);
	if (Released(var.XI_X)) Release(Key.Two);

	if (var.toggleXY and Pressed(var.XI_X)) Press(Key.Three);
	if (var.toggleXY and Released(var.XI_X)) Release(Key.Three);
	
	if (Pressed(var.XI_Y)) Press(Key.Three);
	if (Released(var.XI_Y)) Release(Key.Three);
	
	if (Pressed(var.XI_RB)) Press(Key.Four);
	if (Released(var.XI_RB)) Release(Key.Four);

	if (Pressed(var.XI_B)) {
		Release(Key.LeftShift);
		Press(Key.Escape);
		Release(Key.Escape);
		Press(Key.F9);
		Wait(0.05 s);
		Release(Key.F9);
		Press(Key.PageDown);
		Release(Key.PageDown);

		var.leftCount = 0;
		var.rightCount = 0;
		var.startCount = 0;
	}

	if (Pressed(var.XI_Back)) Press(Key.Tab);
	if (Released(var.XI_Back)) Release(Key.Tab);

	if (Pressed(var.XI_Start)) {
		var.startCount++;
		if (var.startCount == 1) {
			Press(Key.F9);
			Release(Key.F9);
		} else {
			var.startCount = 0;
		}
		var.leftCount = 0;
		var.rightCount = 0;

		Release(Key.LeftShift);
		Press(Key.Escape);
		Release(Key.Escape);
		Mouse.cursorpos = [Window.Left, var.WindowTop] + [var.scaleX * 440, var.scaleY * 660];
		[var.cursorX, var.cursorY] = [var.scaleX * 440, var.scaleY * 660];
		var.RightStickLock = True;
		if (var.debug) DebugPrint("cursorX: " + var.cursorX + ", cursorY: " + var.cursorY);
	}
	if (Pressed(var.XI_Right)) {
		var.rightCount++;
		if (var.rightCount == 1) {
			Press(Key.Escape);
			Release(Key.Escape);
			Press(Key.F9);
			Wait(0.05 s);
			Release(Key.F9);
		} else {
			var.rightCount = 0;
		}
		var.leftCount = 0;
		var.startCount = 0;
	}
	if (Pressed(var.XI_Left)) {
		Press(Key.Escape);
		Release(Key.Escape);
		Press(Key.F9);
		Wait(0.05 s);
		Release(Key.F9);
		var.leftCount++;
		if (var.leftCount == 1) {
			Press(Key.P);
			Release(Key.P);
		} else if (var.leftCount == 2) {
			Press(Key.S);
			Release(Key.S);
		} else if (var.leftCount == 3) {
			Press(Key.F);
			Release(Key.F);
		} else if (var.leftCount == 4) {
			Press(Key.N);
			Release(Key.N);
		} else {
			var.leftCount = 0;
		}
		var.rightCount = 0;
		var.startCount = 0;
	}

	if (Pressed(var.XI_Up)) Press(Key.Enter);
	if (Released(var.XI_Up)) Release(Key.Enter);

	if (Pressed(var.XI_LThumb)) Press(Key.Q);
	if (Released(var.XI_LThumb)) Release(Key.Q);

	if (Pressed(var.XI_Right) and not HeldDown(var.XI_Right, 0.3)) Press(Key.I);
	if (Released(var.XI_Right) and not HeldDown(var.XI_Right, 0.3)) Release(Key.I);
	if (HeldDown(var.XI_Right, 0.3)) {
		Press(Key.T);
		Release(Key.T);
		Press(Key.T);
	}
	if (Released(var.XI_Right)) Release(Key.T);

	if (SingleClicked(var.XI_Down)) {
		Release(Key.LeftShift);
		Press(Key.Escape);
		Release(Key.Escape);
		Press(Key.F9);
		Wait(0.05 s);
		Release(Key.F9);
		Press(Key.PageDown);
		Release(Key.PageDown);

		var.leftCount = 0;
		var.rightCount = 0;
		var.startCount = 0;
	}
	if (DoubleClicked(var.XI_Down)) {
		Press(Key.F9);			// 모든창 닫기
		Release(Key.F9);
		Press(Key.Enter);
		Release(Key.Enter);
		Release(Key.LeftShift);
		TypeUnicode("/p 1");
		Press(Key.Enter);
		Release(Key.Enter);
		if (var.supportMode or var.dealerMode) {
			Press(Key.G);	// 깃발
			Release(Key.G);	// 깃발
			Press(Key.G);	// 깃발
		}
	} else if (Released(var.XI_Down)) {
		Release(Key.G);
	}

	if (Pressed(var.XI_RThumb)) {
	/*
		BeepAsterisk;
		BeepExclamation;
		BeepHand;
		BeepQuestion;
		BeepDefault;
	*/
		BeepHand;
		var.toggleChatMode = True;
		Release(Key.LeftShift);
		Press(Key.printscreen);
		Release(Key.printscreen);
		var.toggleChatMode = False;
	}

	////////////////////////////////////////////////////////////
	// Toggle Mode
	//
	var.toggleSec = 0.5;
	if (Pressed(HeldDown(var.XI_X, var.toggleSec) and HeldDown(var.XI_Y, var.toggleSec))) {
		var.toggleXY = not var.toggleXY;
	} else if (Released(HeldDown(var.XI_X, var.toggleSec) and HeldDown(var.XI_Y, var.toggleSec))) {
		var.toggleX = False;
		var.toggleY = False;
	} else if (var.supportMode) {
		if (var.classMode == var.crusader) {
			if (HeldDown(var.XI_A, var.toggleSec)) {
				var.toggleA = True;
			} else if (Pressed(var.XI_A)) {
				var.toggleA = False;
			}
		} else {
			var.toggleA = var.toggleA xor Pressed(var.XI_A);
		}
		var.toggleX = var.toggleX xor Pressed(var.XI_X);
		var.toggleY = var.toggleY xor Pressed(var.XI_Y);
	} else if (var.dealerMode) {
		var.toggleSec = 5;
		var.toggleA = var.toggleA xor HeldDown(var.XI_A, var.toggleSec);
		var.toggleX = var.toggleX xor HeldDown(var.XI_X, var.toggleSec);
		var.toggleY = var.toggleY xor HeldDown(var.XI_Y, var.toggleSec);
	} else {
		if (HeldDown(var.XI_A, var.toggleSec)) {
			var.toggleA = True;
		} else if (Pressed(var.XI_A)) {
			var.toggleA = False;
		}
		if (HeldDown(var.XI_X, var.toggleSec)) {
			var.toggleX = True;
		} else if (Pressed(var.XI_X)) {
			var.toggleX = False;
		}
		if (HeldDown(var.XI_Y, var.toggleSec)) {
			var.toggleY = True;
		} else if (Pressed(var.XI_Y)) {
			var.toggleY = False;
		}
	}
	if (Pressed(HeldDown(var.XI_RB, var.toggleSec))) {
		var.toggleRB = True;
	} else if (Pressed(var.XI_RB) and (not HeldDown(var.XI_RB, var.toggleSec))) {
		var.toggleRB = False;
	}
	if (DoubleClicked(var.XI_RB)) var.autofireMode = not var.autofireMode;

	if (Pressed(Key.T)) {
		if (var.toggleChatMode == False) {
			var.dealerMode = False;
			var.supportMode = False;
		}
	}

	// rooting mode
	if (var.XI_B) {
		var.toggleRootingMode = True;
	} else if (Released(var.XI_B)) {
		Wait(0.1 s);
		var.toggleRootingMode = False;
	}

	///////////////////////////////////////////////////////
	// Chatting Mode
	var.toggleChatMode = var.toggleChatMode xor Pressed(Key.Enter or var.XI_Up or var.XI_Back);
	if (Pressed(Key.Escape or var.XI_B)) {
		var.toggleChatMode = False;
		var.RightStickLock = False;
	}
	if (abs(Deadzone(var.XI_J1X, var.dzR)) > var.dzDelta or abs(Deadzone(var.XI_J1Y, var.dzR)) > var.dzDelta) {
		if (var.toggleChatMode) {
			Press(Key.Escape);
			Release(Key.Escape);
			Press(Key.F9);
			Release(Key.F9);
			var.toggleChatMode = False;
		}
		var.RightStickLock = False;
	}
}	// (var.LBShift == False)

////////////////////////////////////////////////////////
// auto fire
if (not var.toggleChatMode) {
	var.OnStunMode = var.supportMode and var.autofireMode == False;
	var.OnSupportMode = var.supportMode and var.autofireMode == True;
	var.OnNormalMode = var.OnStunMode == False and var.OnSupportMode == False;
	var.OnFire = (var.toggleSpring or var.dealerMode or not var.OnLeftStick) and not var.XI_B;
	var.OnFireOrWD = var.OnFire or var.classMode >= var.witchDoctor;
	var.OnFireMRB = var.OnFire and var.dealerMode and var.autofireMode and var.OffLeftStick;
	var.toggleYY = (var.toggleX and var.toggleXY) or var.toggleY;
	var.toggleYC = (var.toggleX and var.toggleXY and var.classMode == var.crusader) or var.toggleY;
	var.IsSupportWitchDoctor = var.IsSupportMode and var.classMode >= var.witchDoctor and var.OnRightStick;
	var.IsSupportCrusader = var.IsSupportMode and var.classMode == var.crusader;

	// 개폭
	var.rbmax = 11;
	if (var.OnStunMode and var.IsStunMode) {
		Release(Key.F10);
		if (var.rbnum <= var.rbmax) Press(Mouse.RightButton);
		if (var.rbnum <= var.rbmax) Release(Mouse.RightButton);
		if (var.rbnum < var.rbmax) Wait(var.WaitDogTime s);
		
		if (var.rbnum > var.rbmax and var.classMode >= var.witchDoctor and var.CustomActionKey == True) Press(Key.F7);
		if (var.rbnum > var.rbmax and var.classMode >= var.witchDoctor and var.CustomActionKey == False) Press(Key.Three);
		if (var.rbnum > var.rbmax and var.classMode >= var.witchDoctor) var.IsPressedKey = True;
		if (var.rbnum > var.rbmax and var.classMode >= var.witchDoctor) Wait(0.2 s);
		if (var.IsPressedKey and var.CustomActionKey == True) Release(Key.F7);
		if (var.IsPressedKey and var.CustomActionKey == False) Release(Key.Three);
		if (var.rbnum > var.rbmax) var.rbnum = 0 else var.rbnum++;
		var.IsPressedKey = False;
	}

	// 법성 or 공부
	if (var.OnSupportMode and not (var.classMode == var.crusader and var.XI_RT > var.dzTR)) {
		var.WaitCount = 2;
		var.IsRbNum = var.rbnum < var.rbmax - 1;
		var.SkillStep = var.SkillNum % 3;
		if (var.SkillNum == 99) var.SkillNum = 0 else var.SkillNum++;

		// Right Button
		if (var.IsSupportCrusader) Press(Mouse.RightButton);
		if (var.IsSupportCrusader) Release(Mouse.RightButton);
		if (var.IsSupportCrusader) Wait(var.WaitButtonTime * 2 s);

		if (var.rbnum <= var.rbmax and var.IsSupportWitchDoctor) Press(Mouse.RightButton);
		if (var.rbnum <= var.rbmax and var.IsSupportWitchDoctor) Release(Mouse.RightButton);
		if (var.rbnum <= var.rbmax and var.IsSupportWitchDoctor) Wait(var.WaitButtonTime * 2);

		// One
		if (var.IsRbNum and var.SkillStep == 0 and var.OnFire and var.toggleA and var.CustomActionKey == True) Press(Key.F5);
		if (var.IsRbNum and var.SkillStep == 0 and var.OnFire and var.toggleA and var.CustomActionKey == False) Press(Key.One);
		if (var.IsRbNum and var.SkillStep == 0 and var.OnFire and var.toggleA) var.IsPressedKey = True;
		if (var.IsRbNum and var.SkillStep == 0 and var.OnFire and var.toggleA) Wait(var.WaitButtonTime s);
		if (var.IsPressedKey and var.CustomActionKey == True) Release(Key.F5);
		if (var.IsPressedKey and var.CustomActionKey == False) Release(Key.One);
		if (var.IsRbNum and var.SkillStep == 0 and var.OnFire and var.toggleA) var.WaitCount++;
		var.IsPressedKey = False;

		// Two
		if (var.IsRbNum and var.SkillStep == 1 and var.OnFire and var.toggleX and var.CustomActionKey == True) Press(Key.F6);
		if (var.IsRbNum and var.SkillStep == 1 and var.OnFire and var.toggleX and var.CustomActionKey == False) Press(Key.Two);
		if (var.IsRbNum and var.SkillStep == 1 and var.OnFire and var.toggleX) var.IsPressedKey = True;
		if (var.IsRbNum and var.SkillStep == 1 and var.OnFire and var.toggleX) Wait(var.WaitButtonTime s);
		if (var.IsPressedKey and var.CustomActionKey == True) Release(Key.F6);
		if (var.IsPressedKey and var.CustomActionKey == False) Release(Key.Two);
		if (var.IsRbNum and var.SkillStep == 1 and var.OnFire and var.toggleX) var.WaitCount++;
		var.IsPressedKey = False;

		// Three
		if (var.rbnum > var.rbmax and var.IsSupportWitchDoctor and var.CustomActionKey == True) Press(Key.F7);
		if (var.rbnum > var.rbmax and var.IsSupportWitchDoctor and var.CustomActionKey == False) Press(Key.Three);
		if (var.rbnum > var.rbmax and var.IsSupportWitchDoctor) var.IsPressedKey = True;
		if (var.rbnum > var.rbmax and var.IsSupportWitchDoctor) Wait(0.2 s);
		if (var.IsPressedKey and var.CustomActionKey == True) Release(Key.F7);
		if (var.IsPressedKey and var.CustomActionKey == False) Release(Key.Three);
		if (var.rbnum > var.rbmax) var.rbnum = 0 else if (var.IsSupportWitchDoctor) var.rbnum++;
		var.IsPressedKey = False;

		if (var.OnFire and var.SkillStep == 2 and var.toggleYC and var.CustomActionKey == True) Press(Key.F7);
		if (var.OnFire and var.SkillStep == 2 and var.toggleYC and var.CustomActionKey == False) Press(Key.Three);
		if (var.OnFire and var.SkillStep == 2 and var.toggleYC) var.IsPressedKey = True;
		if (var.OnFire and var.SkillStep == 2 and var.toggleYC) Wait(var.WaitButtonTime s);
		if (var.IsPressedKey and var.CustomActionKey == True) Release(Key.F7);
		if (var.IsPressedKey and var.CustomActionKey == False) Release(Key.Three);
		var.IsPressedKey = False;

		// Four
		if (var.IsRbNum and var.OnFireOrWD and (var.autofireMode or var.toggleRB) and var.CustomActionKey == True) Press(Key.F8);
		if (var.IsRbNum and var.OnFireOrWD and (var.autofireMode or var.toggleRB) and var.CustomActionKey == False) Press(Key.Four);
		if (var.IsRbNum and var.OnFireOrWD and (var.autofireMode or var.toggleRB)) var.IsPressedKey = True;
		if (var.IsRbNum and var.OnFireOrWD and (var.autofireMode or var.toggleRB)) Wait(var.WaitButtonTime s);
		if (var.IsPressedKey and var.CustomActionKey == True) Release(Key.F8);
		if (var.IsPressedKey and var.CustomActionKey == False) Release(Key.Four);
		if (var.IsRbNum and var.OnFireOrWD and (var.autofireMode or var.toggleRB)) var.WaitCount++;
		var.IsPressedKey = False;

		// Wait
		if (var.rbnum < var.rbmax and var.IsSupportWitchDoctor) Wait(var.WaitDogTime - var.WaitButtonTime * var.WaitCount s);
		if (var.IsSupportWitchDoctor or var.IsSupportCrusader) Release(Key.F10);
	}

	// 기본
	if (var.OnNormalMode) {
		var.SkillStep = var.SkillNum % 4;
		if (var.SkillNum == 99) var.SkillNum = 0 else var.SkillNum++;

		// One
		if (var.waitDealerMode > 0) {
			if (var.OnFire and var.OffLeftStick and var.toggleA and var.CustomActionKey == True) Press(Key.F5);
			if (var.OnFire and var.OffLeftStick and var.toggleA and var.CustomActionKey == False) Press(Key.One);
			if (var.OnFire and var.OffLeftStick and var.toggleA and var.CustomActionKey == True) Release(Key.F5);
			if (var.OnFire and var.OffLeftStick and var.toggleA and var.CustomActionKey == False) Release(Key.One);
			if (var.OnFire and var.OffLeftStick and var.toggleA) Wait(var.waitDealerMode s); // 60초 주기
		}
		if (var.SkillStep == 0 and var.OnFire and var.OffLeftStick and var.toggleA and var.waitDealerMode == 0 and var.CustomActionKey == True) Press(Key.F5);
		if (var.SkillStep == 0 and var.OnFire and var.OffLeftStick and var.toggleA and var.waitDealerMode == 0 and var.CustomActionKey == False) Press(Key.One);
		if (var.SkillStep == 0 and var.OnFire and var.OffLeftStick and var.toggleA and var.waitDealerMode == 0) var.IsPressedKey = True;
		if (var.SkillStep == 0 and var.OnFire and var.OffLeftStick and var.toggleA and var.waitDealerMode == 0) Wait(var.WaitButtonTime s);
		if (var.IsPressedKey and var.CustomActionKey == True) Release(Key.F5);
		if (var.IsPressedKey and var.CustomActionKey == False) Release(Key.One);
		var.IsPressedKey = False;

		// Two
		if (var.SkillStep == 1 and var.OnFire and var.OffLeftStick and var.toggleX and var.CustomActionKey == True) Press(Key.F6);
		if (var.SkillStep == 1 and var.OnFire and var.OffLeftStick and var.toggleX and var.CustomActionKey == False) Press(Key.Two);
		if (var.SkillStep == 1 and var.OnFire and var.OffLeftStick and var.toggleX) var.IsPressedKey = True;
		if (var.SkillStep == 1 and var.OnFire and var.OffLeftStick and var.toggleX) Wait(var.WaitButtonTime s);
		if (var.IsPressedKey and var.CustomActionKey == True) Release(Key.F6);
		if (var.IsPressedKey and var.CustomActionKey == False) Release(Key.Two);
		var.IsPressedKey = False;

		// Three
		if (var.SkillStep == 2 and var.OnFire and var.toggleYY and var.CustomActionKey == True) Press(Key.F7);
		if (var.SkillStep == 2 and var.OnFire and var.toggleYY and var.CustomActionKey == False) Press(Key.Three);
		if (var.SkillStep == 2 and var.OnFire and var.toggleYY) var.IsPressedKey = True;
		if (var.SkillStep == 2 and var.OnFire and var.toggleYY) Wait(var.WaitButtonTime s);
		if (var.IsPressedKey and var.CustomActionKey == True) Release(Key.F7);
		if (var.IsPressedKey and var.CustomActionKey == False) Release(Key.Three);
		var.IsPressedKey = False;

		// Four
		if (var.SkillStep == 3 and var.OnFireOrWD and var.toggleRB and var.CustomActionKey == True) Press(Key.F8);
		if (var.SkillStep == 3 and var.OnFireOrWD and var.toggleRB and var.CustomActionKey == False) Press(Key.Four);
		if (var.SkillStep == 3 and var.OnFireOrWD and var.toggleRB) var.IsPressedKey = True;
		if (var.SkillStep == 3 and var.OnFireOrWD and var.toggleRB) Wait(var.WaitButtonTime s);
		if (var.IsPressedKey and var.CustomActionKey == True) Release(Key.F8);
		if (var.IsPressedKey and var.CustomActionKey == False) Release(Key.Four);
		var.IsPressedKey = False;

		// Right Button
		if (var.OnFireMRB) Press(Mouse.RightButton);
		if (var.OnFireMRB) Release(Mouse.RightButton);
		if (var.OnFireMRB) Wait(var.WaitButtonTime s);
	}
}

